{
    "ftp": {
        "ftp_server": "ftp.server.com",
        "userdata": {
            "encryption": false,
            "ftp_username": "username",
            "ftp_password": "password"
        }
    },
    "update": {
        "enabled": true,
        "ftp_path": "updater",
        "exe_name": "name.exe",
        "attempt_count": 20,
        "attempt_timeout": 20,
        "signature_check_disable_key": "aTdW<<9XyeqNM*LS2<"
    },
    "send_data": {
        "enabled": false,
        "local_path": "..\\date",
        "attempt_count": 20,
        "attempt_timeout": 20
    },
    "actions": {
        "at_startup": {
            "enabled": false,
            "file_name": "stop.bat",
            "timeout": 15
        },
        "at_completion": {
            "enabled": false,
            "file_name": "start.bat"
        }
    },
    "logs": {
        "level": "info",
        "path": "..\\logs",
        "clear_days": 7
    }
}